package SecondSaturday;

public class SecondSaturday {
  void nTo1(int n) {
	  for(int i=n;i>=1;i--) {
		  System.out.println(i);
	  }
  }
  
  void square() {
	  for(int i=1;i<=10;i++) {
		  System.out.println("Square of "+i+" is: "+i*i);
	  }
  }
  void Charcters() {
	  
	  for(int i=65;i<=90;i++) {
		  char ch= (char)+i;
		  System.out.println(ch);
	  }
  }
	void Charcters1() {
		  
		  for(int i=97;i<=122;i++) {
			  char ch= (char)+i;
			  System.out.println(ch);
		  }
	  }
	void digits0To9() {
		for(int i=0;i<=9;i++) {
			System.out.println(i);
		}
	}
	void factors(int n) {
		for(int i=1;i<=n;i++) {
			
				if(n%i==0) {
					System.out.println(i);
				}
			
		}
	}
	void SumOfEven(int n) {
		int res=0;
		int sum=0;
		while(n>0) {
			res=n%10;
			if(res%2==0) {
				sum=sum+res;
			}
			n=n/10;
		}
		System.out.println(sum);
	}
}
