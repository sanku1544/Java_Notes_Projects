package com.tks.Nov29Session;

import java.util.Scanner;

public class Test {
	void m1() {
		Practice p1=new Practice();
		p1.sum(10,50);
		p1.sale(4500, 520);
		p1.avrage(45, 52, 21);
		p1.operations(80, 40);
		p1.table(5);
		p1.cube(11);
		p1.swap(5, 3);
		p1.ascii('A');
		p1.gen();
		p1.odd();
		p1.increment();
	}

	public static void main(String[] args) {
		
		Practice p1=new Practice();
		
		
		Scanner sc=new Scanner(System.in);
		int pos=0;
		int zero=0;
		int neg=0;
		for(int i=1;i<=10;i++) {
			System.out.println("Enter "+i+"no: ");
			int a=sc.nextInt();
			if(a>0) {
				pos++;
			}
			else if(a<0) {
				neg++;
			}else {
				zero++;
			}
		}
		System.out.println("Positive no.s: "+pos);
		System.out.println("Negative no.s: "+neg);
		System.out.println("Zero no.s: "+zero);
	}

}
