package SecondSaturday;

public class SecondSaturday {
  void nTo1(int n) {
	  for(int i=n;i>=1;i--) {
		  System.out.println(i);
	  }
  }
  
  void square() {
	  for(int i=1;i<=10;i++) {
		  System.out.println("Square of "+i+" is: "+i*i);
	  }
  }
  void Charcters() {
	  
	  for(int i=65;i<=90;i++) {
		  char ch= (char)+i;
		  System.out.println(ch);
	  }
  }
	void Charcters1() {
		  
		  for(int i=97;i<=122;i++) {
			  char ch= (char)+i;
			  System.out.println(ch);
		  }
	  }
	void digits0To9() {
		for(int i=0;i<=9;i++) {
			System.out.println(i);
		}
	}
	void factors(int n) {
		for(int i=1;i<=n;i++) {
			
				if(n%i==0) {
					System.out.println(i);
				}
			
		}
	}
	void SumOfEven(int n) {
		int res=0;
		int sum=0;
		while(n>0) {
			res=n%10;
			if(res%2==0) {
				sum=sum+res;
			}
			n=n/10;
		}
		System.out.println("Sum of even  no.s: "+sum);
	}
	void SumOfOdd(int n) {
		int res=0;
		int sum=0;
		while(n>0) {
			res=n%10;
			if(res%2!=0) {
				sum=sum+res;
			}
			n=n/10;
		}
		System.out.println("Sum of odd  no.s: "+sum);
	}
	void table(int t) {
		for(int i=1;i<=10;i++) {
			System.out.println(t+" * "+i+" = "+i*t);
		}
	}
	void series(int n) {
		for(int i=2;i<=n;i+=2) {
			System.out.println(i);
		}
	}
	
	void series2(int n) {
		int sum=0;
		for(int i=1;i<=n;i++) {
			if(i%2==0) {
				sum=0-i;
				System.out.println(sum);
			}else {
				
				System.out.println(i);
			}
		}
	}
	void series3(int n) {
		int series=1;
		for(int i=1;i<=n;i++) {
			series=series*10;
			System.out.println(series);
		}
	}
	
	void series4(int n) {
		int a=0;
		for(int i=1;i<=n;i++) {
			a+=i;
			System.out.println(a);
		}
	}
	void prime() {
		
		for(int i=1;i<=100;i++) {
			int count=0;
			for(int j=1;j<=i;j++) {
				if(i%j==0) {
					count++;
				}
			}
			if(count==2) {
				System.out.println(i);
				}
		}
	}
	
	void seriesSquares(int n) {
		for(int i=1;i<=n;i++) {
			System.out.println(i*i);
		}
	}
	void seriesPlus3(int n) {
		for(int i=3;i<=n;i+=3) {
			System.out.println(i);
		}
	}
	void Fibo(int n) {
		int a=1;
		int b=0;
		System.out.println(b);
		for(int i=1;i<=n;i++) {
			int a2=a+b;
			System.out.println(a2);
			a=b;
			b=a2;
		}
	}
	
	void rev(int n) {
		for(int i=1;i<=n;i++) {
			System.out.print(i+" ");
		}
		for(int i=n;i>=1;i--) {
			  System.out.print(i+" ");
		  }
	}
	
	void rev1(int n) {
		
			int series=1;
			for(int i=1;i<=n;i++) {
				series=series*10;
				System.out.print(series+" ");
			}
			for(int i=n;i>=series;i--) {
				  System.out.print(i+" ");
			  }
		
		
	}
	
	
	
}
