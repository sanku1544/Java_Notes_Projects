package SecondSaturday;

import java.util.Scanner;

public class SecondSaturday {
  void nTo32(int n) {
	  for(int i=n;i>=1;i--) {
		  System.out.println(i);
	  }
  }
  
  void square33() {
	  for(int i=1;i<=10;i++) {
		  System.out.println("Square of "+i+" is: "+i*i);
	  }
  }
  void Charcters34() {
	  
	  for(int i=65;i<=90;i++) {
		  char ch= (char)+i;
		  System.out.println(ch);
	  }
  }
	void Charcters35() {
		  
		  for(int i=97;i<=122;i++) {
			  char ch= (char)+i;
			  System.out.println(ch);
		  }
	  }
	void digits36() {
		for(int i=0;i<=9;i++) {
			System.out.println(i);
		}
	}
	void factors37(int n) {
		for(int i=1;i<=n;i++) {
			
				if(n%i==0) {
					System.out.println(i);
				}
			
		}
	}
	void SumOfEven38(int n) {
		int res=0;
		int sum=0;
		while(n>0) {
			res=n%10;
			if(res%2==0) {
				sum=sum+res; 
			}
			n=n/10;
		}
		System.out.println("Sum of even  no.s: "+sum);
	}
	void SumOfOdd39(int n) {
		int res=0;
		int sum=0;
		while(n>0) {
			res=n%10;
			if(res%2!=0) {
				sum=sum+res;
			}
			n=n/10;
		}
		System.out.println("Sum of odd  no.s: "+sum);
	}
	void table40(int t) {
		for(int i=1;i<=10;i++) {
			System.out.println(t+" * "+i+" = "+i*t);
		}
	}
	void series42(int n) {
		for(int i=2;i<=n;i+=2) {
			System.out.println(i);
		}
	}
	
	void series43(int n) {
		int sum=0;
		for(int i=1;i<=n;i++) {
			if(i%2==0) {
				sum=0-i;
				System.out.println(sum);
			}else {
				
				System.out.println(i);
			}
		}
	}
	void series44(int n) {
		int series=1;
		for(int i=1;i<=n;i++) {
			series=series*10;
			System.out.println(series);
		}
	}
	
	void series45(int n) {
		int a=0;
		for(int i=1;i<=n;i++) {
			a+=i;
			System.out.println(a);
		}
	}
	void prime46() {
		
		for(int i=1;i<=100;i++) {
			int count=0;
			for(int j=1;j<=i;j++) {
				if(i%j==0) {
					count++;
				}
			}
			if(count==2) {
				System.out.println(i);
				}
		}
	}
	
	void seriesSquares47(int n) {
		for(int i=1;i<=n;i++) {
			System.out.println(i*i);
		}
	}
	void seriesPlus48(int n) {
		for(int i=3;i<=n;i+=3) {
			System.out.println(i);
		}
	}
	void Fibo49(int n) {
		int a=1;
		int b=0;
		System.out.println(b);
		for(int i=1;i<=n;i++) {
			int a2=a+b;
			System.out.println(a2);
			a=b;
			b=a2;
		}
	}
	
	void rev50(int n) {
		for(int i=1;i<=n;i++) {
			System.out.print(i+" ");
		}
		for(int i=n;i>=1;i--) {
			  System.out.print(i+" ");
		  }
	}
	
	void rev51(int n) {
		
			int series=1;
			for(int i=1;i<=n;i++) {
				series=series*10;
				System.out.print(series+" ");
			}
			for(int i=n-1;i>=1;i--) {
				series=series/10;
				  System.out.print(series+" ");
			  }
		
		
	}
	void confuse52(int n) {
		int no=3;
		for(int i=2;i<=n;i+=2) {
			System.out.print(i+" "+i/2+" "+no+" ");
			no=no*3;
		}
	}
	void fact53(int n) {
		int fact=1;
		for(int i=1;i<=n;i++) {
			fact=fact*i;
			System.out.print(fact+" ");
		}
	}	
		void sumOfFact54(int n) {
			int fact=1;
			int sum=0;
			for(int i=1;i<=n;i++) {
				fact=fact*i;
				sum=fact+sum;
		}
			System.out.println(sum);
	}
		
		void InputCount55() {
			Scanner sc=new Scanner(System.in);
			int pos=0;
			int neg=0;
			int zero=0;
			for(int i=1;i<=10;i++) {
				System.out.println("Enter "+i+" no: ");
				int a=sc.nextInt();
				if(a>0) {
					pos++;
				}else if(a<0) {
					neg++;
				}else {
					zero++;
				}
			}
			System.out.println("Positive no.s are: "+pos);
			System.out.println("Negative no.s are: "+neg);
			System.out.println("Zero no.s are: "+zero);
		}
	
		void CountLargeSmall56() {
			Scanner sc=new Scanner(System.in);
			int large=0;
			int small=0;
			for(int i=1;i<=10;i++) {
				System.out.println("Enter "+i+" no: ");
				int a=sc.nextInt();

				if(large<a) {
					large=a;
					small=large;
				}
				else if(small>a){
					small=a;
				}
			}
			System.out.println("Large no.s is: "+large);
			System.out.println("Small no.s is: "+small);
			
		}
	
	
}
