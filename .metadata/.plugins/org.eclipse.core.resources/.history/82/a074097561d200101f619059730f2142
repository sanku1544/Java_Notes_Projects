package SecondSaturday;

public class SecondSaturday {
  void nTo1(int n) {
	  for(int i=n;i>=1;i--) {
		  System.out.println(i);
	  }
  }
  
  void square() {
	  for(int i=1;i<=10;i++) {
		  System.out.println("Square of "+i+" is: "+i*i);
	  }
  }
  void Charcters() {
	  
	  for(int i=65;i<=90;i++) {
		  char ch= (char)+i;
		  System.out.println(ch);
	  }
  }
	void Charcters1() {
		  
		  for(int i=95;i<=121;i++) {
			  char ch= (char)+i;
			  System.out.println(ch);
		  }
	  }
}
